{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "client/dist",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/client/dist/$1"
        }
    ],
    "functions": {
        "api/index.js": {
            "maxDuration": 30
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}